{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "member-access": [true, "no-public"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ]
  }
}
